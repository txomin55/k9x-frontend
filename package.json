{
  "name": "k9x-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "ui/*",
    "configuration/*",
    "coverage_processor/*"
  ],
  "scripts": {
    "build": "turbo run build -- --mode ${CI_BUILD_ENV}",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{cjs,js,jsx,md,mdx}\"",
    "test:unit": "turbo run test:unit --parallel",
    "test:unit:coverage": "turbo run test:unit:coverage --parallel",
    "unit:coverage-merge": "node coverage_processor/unit_coverage_processor.cjs",
    "unit:junit-merge": "node coverage_processor/unit_jUnit_processor.cjs",
    "test:e2e": "pnpm --filter ./ui/app test:e2e",
    "total:coverage-merge": "node coverage_processor/total_coverage_processor.cjs",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.1",
    "jsdom": "27.2.0",
    "junit-report-merger": "^9.0.3",
    "prettier": "3.7.4",
    "regenerator-runtime": "^0.14.1",
    "turbo": "^2.8.10",
    "vite": "7.2.6",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^4.0.15",
    "vitest-sonar-reporter": "^3.0.0"
  },
  "dependencies": {
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "packageManager": "pnpm@10.30.0"
}
