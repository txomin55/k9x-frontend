{
  "name": "app",
  "version": "0.0.0",
  "type": "module",
  "description": "",
  "scripts": {
    "dev": "pnpm run start:offline",
    "start:offline": "pnpm run mock-server & vite --mode offline",
    "start:integrated": "vite --mode integrated",
    "start:develop": "vite --mode develop",
    "build": "vite build",
    "serve": "vite preview",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest --coverage",
    "test:e2e": "pnpm run start:offline & VITE_APP_IS_E2E=true cypress run --spec tests/e2e/specs/**/*.js --browser chrome && pnpm run test:functional-coverage",
    "test:e2e:dashboard": "pnpm run start:offline & VITE_APP_IS_E2E=true cypress open",
    "test:e2e:real": "pnpm run start:offline & VITE_APP_IS_E2E=true cypress run --record --key ${CY_E2E_KEY} --spec tests/e2e/specs/**/*.js --browser chrome",
    "test:functional-coverage": "pnpm nyc report --reporter=text-summary",
    "test:smoke": "VITE_APP_IS_E2E=true cypress run --spec tests/e2e/specs/**/*.js",
    "local:test:e2e": "pnpm run start:offline & VITE_APP_IS_E2E=true cypress run --spec tests/e2e/specs/**/*.js --browser chrome",
    "local:e2e:dashboard": "pnpm run start:offline & VITE_APP_IS_E2E=true cypress open",
    "mock-server": "prism mock -d public/openapi.json"
  },
  "license": "MIT",
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.7",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@stoplight/prism-cli": "^5.14.2",
    "@testing-library/jest-dom": "^6.9.1",
    "crypto-browserify": "^3.12.1",
    "cypress": "^14.5.4",
    "cypress-cdp-coverage": "^1.0.0",
    "cypress-multi-reporters": "^2.0.5",
    "cypress-sonarqube-reporter": "^1.13.6",
    "eslint-plugin-cypress": "^4.3.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "mocha-junit-reporter": "^2.2.1",
    "my-vitest": "workspace:^",
    "nyc": "^17.1.0",
    "require-context": "^1.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "vite-jsconfig-paths": "^2.0.1",
    "vite-plugin-commonjs": "^0.10.4",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-pwa": "^0.21.2",
    "vite-plugin-require-transform": "^1.0.21",
    "workbox-precaching": "^7.4.0",
    "workbox-window": "^7.4.0"
  },
  "dependencies": {
    "axios": "1.13.2",
    "library": "workspace:^",
    "i18next": "^24.2.3",
    "openapi-client-axios": "7.5.4",
    "react-i18next": "^15.7.4",
    "react-router-dom": "^7.10.1",
    "vite-plugin-static-copy": "^3.1.4",
    "workbox-core": "^7.4.0",
    "zustand": "^5.0.9"
  }
}
